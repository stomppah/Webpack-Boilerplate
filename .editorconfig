# To learn more about .editorconfig see https://aka.ms/editorconfigdocs
root = true

# All files
[*]
indent_style = space
indent_size = 4
end_of_line = lf
charset = utf-8
trim_trailing_whitespaces = true
insert_final_newline = true

# Xml files
[*.xml]
indent_size = 2

[*.md]
trim_trailing_whitespaces = false
